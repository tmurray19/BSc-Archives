<Group labelreference="L16" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Newflows:=proc(G)</Text-field>
<Text-field style="Text" layout="Normal">A procedure to modify original flows on G along SinkPaths with minimal cost</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">local arc,path,node,i,j,k,n,Flow,Npaths,cost,eps,mincost,bestpath,besteps,epset;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">global Nset,cp,lp,SinkPaths,phi,c,lc,ArcSign;  </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">  SourceSink(Gp);</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">  if SinkPaths={} then </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">   Links(G);Flow:=0;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">    for node in Out[1] do</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">       Flow:=Flow+phi[1,node];</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">    od;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">    lprint (`Maximal flow found`,Flow); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">  else   </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">     for k from 1 to  nops(SinkPaths) do</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">        cost:=0; epset:={};path:=op(k,SinkPaths);</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">        for n from 1 to nops(path)-1 do </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">            i:= op(n,path); j:=op(n+1,path);</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">            epset:=epset union {cp[i,j]};</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">            cost:=lp[i,j]+cost;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">        od:</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">        eps:=min(op(epset));</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">        if k=1 then </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">           mincost:=cost;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">           bestpath:=path;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">           besteps:=eps;  </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">        elif mincost&gt;cost then </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">           mincost:=cost; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">           bestpath:=path;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">           besteps:=eps;  </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">        fi;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">       od:</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">   lprint(`The best path in Gp is `,bestpath);
   lprint(`epsilon, the min capacity on this path, is `,eps);
   lprint(`min cost is `,mincost);</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">   for k from 1 to nops(bestpath)-1 do</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">     i:=op(k,bestpath): j:=op(k+1,bestpath):</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">     if ArcSign[i,j]=1 then</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">        phi[i,j]:=phi[i,j]+besteps:</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">     else</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">        phi[j,i]:=phi[j,i]-besteps:</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">    fi; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">    od;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">fi;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">end:</Text-field>
</Input>
</Group>
<Group labelreference="L17" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
</Input>
