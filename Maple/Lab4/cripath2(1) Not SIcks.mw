<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">CritPath:=proc(G,T)</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">local p,newe,critpath,start,i,j,v;</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">global e,l,f,In,Out,N;</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1"># N, number of activities</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">N:=max(seq(op(op(i,G)),i=1..nops(G)))-1;</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1"># </Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1"># Vertices In and Out of a given one</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">#</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">In:=table();Out:=table();</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">for i from 0 to N+1 do</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  In[i]:={};Out[i]:={};</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">od:</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">for v in G do</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  i:=op(1,v);j:=op(2,v);</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  In[j]:=In[j] union {i}; Out[i]:=Out[i] union {j};</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">od:</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">#</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1"># Forward scan</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">#</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">T[0]:=0;T[N+1]:=0;</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1"># e gives the earliest starting times </Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">e:=table();p:=table();</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">e[0]:=0;p[0]:=0;</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">for j from 1 to N+1 do </Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  v:=op(1,In[j]);e[j]:=e[v]+T[v];p[j]:=v;</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  for v in In[j] do</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">    newe:=e[v]+T[v];</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">    if newe &gt;=e[j] then </Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">      e[j]:=newe;p[j]:=v;</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">    fi:</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  od:</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">od:</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">#</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1"># Backward Scan</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">#</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">critpath:=N+1;start:=N+1;</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">while not start = 0 do </Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  start:=p[start];critpath:=start,critpath;</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">od:</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">critpath:=[critpath];
print(`The Total amount of work to be completed is:`, add(T[jj],jj=1..N));</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">print(`The Critical Path is:`, critpath);</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">print(`The minimum completion time with sufficient workers is:`, e[N+1]);</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">#</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1"># Latest starting times</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">#</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">l:=table();f:=table();f[N+1]:=0;l[N+1]:=e[N+1];</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">for i from N by -1 to 0 do</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  l[i]:=min(op(map(v-&gt;l[v],Out[i])))-T[i];f[i]:=l[i]-e[i];</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">od:</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">print(`Activity Earliest Latest Float`);</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">for i from 0 to N+1 do</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  print(i,`   `,e[i],`    `,l[i],`   `,f[i]);</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">od:</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">end:</Text-field>
</Input>
</Group>
