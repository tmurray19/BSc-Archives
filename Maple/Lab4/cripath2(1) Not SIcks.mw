<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">CritSchedule:=proc(G,T,Nw)</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">#</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1"># This procedure finds the optimal schedule </Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1"># according to the critical path scheduling algorithm
# for Nw workers. Any idle worker is always assigned 
# an activity if possible.</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">#</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">local Tset,Tot,k,kk,i,Topt,Acts,Current,Done,
Next,Time,DelT,Tleft,Nextlist,noc,v,w,workertaskseq,
workerv,availset,Activity,nextjob,workeri,schedulelist;</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">global N,In,Out,e;
# first find critical path parameters
CritPath(G,T);</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1"># Total time for all activities</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">Tot:=sum(T[k],k=1..N);</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1"># Optimal time assuming precedence relations can be satisfied</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">Topt:=max(Tot/Nw,e[N+1]);</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">Acts:={seq(k,k=1..N+1)};
# sequences of tasks undertaken by worker i
workertaskseq:=array(1..Nw):
for i from 1 to Nw do
 workertaskseq[i]:=op({});
od:
#set of workers available
availset:={seq(kk,kk=1..Nw)};
#array giving worker assigned to activity k
Activity:=array(1..N):  </Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">#</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1"># The activities Done, Current and Next</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">#</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">Current:={};Done:={};Next:=Out[0];Time:=0;</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">#</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1"># The Time left for activities</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">#</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">Tleft:=table(); </Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  for i from 0 to N+1 do</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  Tleft[i]:=T[i];</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">od:</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">while nops(Done)&lt;&gt;N do</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">#</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1"># Assign activities to available workers:</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">#</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1"> Nextlist:=RankSet1(Next);</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  noc:=nops(Current);</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  for i from 1 to min(Nw-noc,nops(Next)) do
    nextjob:=op(i,Nextlist);
    workeri:=op(1,availset);# first available worker is chosen
    Activity[nextjob]:=workeri;
    availset:=availset minus {workeri};
    workertaskseq[workeri]:=workertaskseq[workeri],Time,nextjob;</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">    Current:=Current union {nextjob};</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">    Next:=Next minus {nextjob};</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  od:</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  lprint(`Time=`,Time,`Current activities=`,op(Current));</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  Tset:={};</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  for v in Current do </Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">    Tset:=Tset union {Tleft[v]};</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  od:</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1"># Time before next activity completed </Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1"> DelT:=min(op(Tset));</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  Time:=Time+DelT;</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  for v in Current do</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">    Tleft[v]:=Tleft[v]-DelT;</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">    if Tleft[v]=0 then</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">      lprint(`Time=`,Time,`Activity`,v,`completed`);
      workerv:=Activity[v];
      workertaskseq[workerv]:=workertaskseq[workerv],Time,v;
      availset:=availset union {workerv};</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">      Done:=Done union {v};Current:=Current minus {v};</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">      for w in Out[v] do</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">         if In[w] minus Done = {} then </Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">           Next:=Next union {w} minus {N+1}:</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">         fi:</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">     od:</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">    fi:</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  od:</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">od:</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">if Time = Topt then </Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  lprint();</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">  lprint(`****** Optimal Schedule Found for `,Nw,` Workers! *****`);
  if Topt=e[N+1] then lprint(`****** The Minimal Completion Time of `,e[N+1],` is achieved for `,Nw,` Workers! *****`);end if;</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1">fi:
schedulelist:=[seq([workertaskseq[k]],k=1..Nw)];
#print(schedulelist);
GraphSchedule(schedulelist);
end:</Text-field>
<Text-field prompt="&gt; " style="_cstyle1" layout="_pstyle1"></Text-field>
</Input>
</Group>
