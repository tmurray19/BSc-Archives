<Group labelreference="L51" drawlabel="true">
<Input><Text-field prompt="&gt; " style="2D Input" executable="false" layout="_pstyle1" linespacing="0.0" initial="0" firstindent="0" leftmargin="0" layout_style_name="_pstyle1" pagebreak-before="false" spaceabove="0" linebreak="space" rightmargin="0" bulletsuffix="" alignment="left" bullet="none" spacebelow="0"><Font executable="false">Activity:=proc(Act,Time,Prec)
global G,T;
local FinInd,LabeliA,LabelAi,iA,NewP,Acurrent,newA,Ai,AA,AB,iAA,iAB,
Alist,FinNode,NewA,NewAlist,i,arc;
FinInd:=nops(Act)+1;
T:=array(0..FinInd);T[0]:=0;T[FinInd]:=0;
LabelAi:=table();LabeliA:=table();
LabelAi[0]:=&quot;Start&quot;;LabeliA[&quot;Start&quot;]:=0;
iA:=0;
NewP:=table();
for AA in Act do 
  NewP[AA]:=Prec[AA];
end do;
Acurrent:=Act;
Alist:=[0];#ordered activities
while Acurrent&lt;&gt;{} do
NewA:={};
for Ai in Acurrent do
 if NewP[Ai]={} then NewA:=NewA union {Ai}; end if;
end do;
NewAlist:={};
for i from 1 to nops(NewA) do
  LabelAi[i+iA]:=op(i,NewA);
  LabeliA[op(i,NewA)]:=i+iA;
  NewAlist:=NewAlist union {i+iA};
  T[i+iA]:=Time[op(i,NewA)]; 
end do;
Alist:=Alist,[seq(op(nops(NewAlist)-i,NewAlist),i=0..nops(NewAlist)-1)];
iA:=iA+nops(NewA);
Acurrent:=Acurrent minus NewA;
for Ai in Act do
 NewP[Ai]:=NewP[Ai] minus NewA;
end do;
end do:
LabelAi[FinInd]:=&quot;Finish&quot;;
LabeliA[&quot;Finish&quot;]:=FinInd;
Alist:=Alist,[FinInd];
G:={};FinNode:={seq(i,i=1..FinInd-1)};
for AA in Act do
 iAA:=LabeliA[AA]: 
 if Prec[AA]={} then 
   G:=G union {[0,iAA]};
 else  
   for AB in Prec[AA] do
      iAB:=LabeliA[AB];
      FinNode:=FinNode minus {iAB};
      G:=G union {[iAB,iAA]};
    end do:
 end if;
end do:
for iA in FinNode do
  G:=G union {[iA,FinInd]};
end do:
new(Graph):
addvertex(seq(i,i=0..FinInd),Graph):
for arc in G do
  connect([arc[1]],[arc[2]],Graph);
end do;
print(`The Activities have been relabeled with completion times:`);
for i from 0 to FinInd do
  print(`Activity `,LabelAi[i],` is activity `,i,` with completion time `,T[i]);
end do;
print(`The following is a graph of the Activity network:`);
print(draw(Linear(Alist),Graph));
print(`The Activity Network is described by the set G:`); 
G;
end proc:</Font></Text-field>
</Input>
</Group>
